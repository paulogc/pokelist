<h1>View</h1>

<div class="form-horizontal">
  <div class="form-group">
    <label class="col-sm-2 control-label">Name</label>
    <div class="col-sm-10">
      <label class="form-control">{{model.name}}</label>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-10">
      <img src="{{model.image}}" />
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Abilities:</label>
    <div class="col-sm-10">
      {{#each model.abilities as |ability|}}
        <label class="form-control">{{ability.name}}</label>
      {{/each}}
    </div>
  </div>
</div>